<mvc:View xmlns:mvc="sap.ui.core.mvc" controllerName="com.sap.build.standard.adminEngine.controller.MasterPage1" xmlns="sap.m">
    <Page title="Atividades" showHeader="true" showFooter="true" showNavButton="false">
        <content>
            <List items="{/List}" id="idList" mode="None" width="100%" headerText="Header" footerText="" backgroundDesign="Solid" showSeparators="All" growing="true" growingThreshold="20" growingScrollToLoad="true" visible="true">
                <infoToolbar/>
                <headerToolbar>
                    <Toolbar enabled="true" visible="true" width="" design="Solid">
                        <content>
                            <SearchField placeholder="Search" liveChange="onSearch" visible="true" width="100%"/>
                        </content>
                    </Toolbar>
                </headerToolbar>
                <items>
                    <ObjectListItem intro="" title="{mainText}" number="" numberUnit="" numberState="None" type="Navigation" selected="false" showMarkers="false" press="_onObjectListItemPress">
                        <attributes>
                            <ObjectAttribute title="" text="{subText}" active="false" visible="true"/>
                        </attributes>
                        <firstStatus/>
                        <secondStatus/>
                    </ObjectListItem>
                </items>
            </List>
        </content>
        <footer>
            <OverflowToolbar width="100%" height="46px" design="Auto" enabled="true" visible="true">
                <content/>
            </OverflowToolbar>
        </footer>
        <headerContent/>
    </Page>
</mvc:View>
