                                <form:fields>
                                    <ComboBox editable="true" enabled="true" visible="true" width="auto" valueState="None" maxWidth="100%">
                                        <items>
                                            <core:Item text="CD Claro" enabled="true" key="CD Claro"/>
                                            <core:Item text="Cajamar" enabled="true" key="Cajamar"/>
                                            <core:Item text="Teste" enabled="true" key="Teste"/>
                                        </items>
                                    </ComboBox>
                                </form:fields>
                            </form:FormElement>
                        </form:formElements>
                        <form:title/>
